<div class="page-header">
  <h1>Clients</h1>
</div>
<div class="table-title">
  <%= link_to "New Client", new_client_path %>
</div>
<table class="table-borders wide-table">
  <% @clients.each do |client| %>
    <tr>
      <td>
        <div class="clients">
          <%= link_to client.name, client %>
        </div>
      </td>
    </tr>
  <% end %>
</table>
